{
  "name": "react-website",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "xo && stylelint src/*.css src/components/**/*.css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --display-error-details",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "prepush": "npm run lint"
  },
  "xo": {
    "extends": "xo-react",
    "parser": "babel-eslint",
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "unicorn/filename-case": "off",
      "padding-line-between-statements": [
        "error",
        {
          "blankLine": "any",
          "prev": "*",
          "next": "*"
        }
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "eol-last": 0,
      "import/no-unassigned-import": 0,
      "react/prop-types": 0,
      "unicorn/no-abusive-eslint-disable": 0
    },
    "ignores": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alix2018/react-website.git"
  },
  "author": "Stephanie Alix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alix2018/react-website/issues"
  },
  "browserslist": [
    "Chrome >= 71",
    "Safari >= 11",
    "Firefox >= 64",
    "Edge >= 17",
    "ios_saf >= 11.3",
    "and_chr >= 68",
    "and_ff >= 65",
    "not ie <= 11"
  ],
  "homepage": "https://github.com/alix2018/react-website#readme",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "clean-webpack-plugin": "2.0.1",
    "eslint-config-xo-react": "0.19.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "stylelint-config-standard": "18.3.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "xo": "0.24.0"
  },
  "dependencies": {
    "@babel/preset-env": "7.4.2",
    "autoprefixer": "9.5.1",
    "babel-loader": "8.0.5",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "5.0.2",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "file-loader": "3.0.1",
    "hookrouter": "1.2.3",
    "html-react-parser": "0.9.1",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.1",
    "postcss-nesting": "7.0.0",
    "postcss-preset-env": "6.6.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-ga": "2.6.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.2.1",
    "webpack": "4.29.6",
    "webpack-merge": "4.2.2"
  }
}
